package findnum;

public class Main {

	public static void main(String[] args) {
		int[] sayilar = new int[]{1,2,86,41,5,86,48,1}
		int aranacak = 5;
		boolean varmi=false;
		for(int sayi:sayilar) {
			if(sayi==aranacak) {
				varmi=true;
				break;			
				}
		}
		if(varmi=true) {
		System.out.println("sayı mevcuttur.");
		}
		else {
			System.out.println("sayı mevcut değildir.");
		}
	}
}